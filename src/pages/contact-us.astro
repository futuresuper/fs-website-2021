---
import BaseLayout from "@layouts/BaseLayout.astro";
import ContactUs from "@components/page/ContactUs.svelte";

let contactPage = await Astro.glob("@pages/contact/contact-us.md");
const content = contactPage[0];
---

<BaseLayout
  title={content.title}
  description={content.description}
  pageDescription={content.intro}
>
  <ContactUs contact={content} />
</BaseLayout>
