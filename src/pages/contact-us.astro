---
import BaseLayout from "@layouts/BaseLayout.astro";
import ContactUs from "@components/page/ContactUs.svelte";

let contactPage = await Astro.glob("../contact/contact-us.md");
const content = contactPage[0];
---

<BaseLayout
  title={content.frontmatter.title}
  description={content.frontmatter.description}
  pageDescription={content.frontmatter.intro}
>
  <ContactUs contact={content} />
</BaseLayout>
